<%= simple_form_for @pokemon do |f| %>
  <%= f.input :name %>
  <%= f.input :level %>
  <%= f.input :abilities %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :photo, as: :file %>
  <%= f.submit 'Generate New Pokemon', class: "btn btn-warning" %>
  <%= link_to "Cancel", root_path, class: "btn btn-danger" %>
<% end %>
