
<div class="container">
  <div class="row">
    <div class="col-6">
      <h1> All pokemons !!! </h1>
      <% @pokemons.each do |pokemon| %>
        <%= link_to pokemon.name, pokemon_path(pokemon) %>
      <% end %>
      <br>
      <%= link_to 'New Pokemon', new_pokemon_path, class: "login-btn" %>
    </div>
</div>
